bash <(curl -Ls https://raw.githubusercontent.com/XrayR-project/XrayR-release/master/install.sh) && sudo curl -o /etc/XrayR/config.yml https://raw.githubusercontent.com/latonyahigr/xrayr/main/GAOJI-USA && cd /etc/XrayR/ && sudo wget -O custom_outbound.json https://raw.githubusercontent.com/latonyahigr/dns/main/US/custom_outbound.json && sudo wget -O route.json https://raw.githubusercontent.com/latonyahigr/dns/main/US/route.json && sudo XrayR restart
